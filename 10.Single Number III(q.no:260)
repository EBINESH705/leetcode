class Solution {
    public int[] singleNumber(int[] nums) {
        int[] result = new int[2];
        int n = nums.length;
        int index = 0;
        for (int i = 0; i < n; i++) {
            boolean found = false;
            for (int j = 0; j < n; j++) {
                if (i != j && nums[i] == nums[j]) {
                    found = true;
                    break;
                }
            }
            if (!found) {
                result[index++] = nums[i];
            }
        }
        return result;
    }
}